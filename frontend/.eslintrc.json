{
   "env": {
      "browser": true,
      "es6": true
   },
   "extends": [
      "plugin:react/recommended",
      "prettier",
      "plugin:@typescript-eslint/recommended"
   ],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true,
         "arrowFunctions": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["react", "prettier"],
   "rules": {
      "prettier/prettier": ["error"]
   }
}
